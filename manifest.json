{
  "manifest_version": 2,
  "name" : "Latest",
  "description": "最新話を常にメモしておこう",
  "version": "1",
  "options_page" : "option.html",
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_title": "Latest",
    "default_popup" : "index.html"
  },
  "background" : {
    "scripts" : ["background.js"],
    "persistent" : false
  },
  "permissions" : ["tabs","storage"]
}
